{{#if isFirstPreference}}
  <span {{bind-attr class="isFirstPreference:party-name:secondary-party-name"}}>{{partyAbbreviation}}</span>
{{else}}
  {{#ic-modal-trigger controls="modal" on-toggle="preferenceClicked"}}
    <span {{bind-attr class="isFirstPreference:party-name:secondary-party-name"}}>{{partyAbbreviation}}</span>
  {{/ic-modal-trigger}}
{{/if}}
